{
  "name": "p5-typescript-template",
  "version": "1.0.0",
  "description": "Template for typescript based p5js projects.",
  "author": "Willyham",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "start-release": "parcel release.html --open",
    "release": "parcel build release.html"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/p5": "^1.4.0",
    "parcel": "^2.3.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "p5": "^1.4.1"
  },
  "targets": {
    "release": {
      "includeNodeModules": {
        "p5": false
      }
    }
  }
}
